<h1>Listing projects</h1>

<%= link_to 'New project', new_project_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>

  <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.description %></td>
      </tr>
  <% end %>
</table>